{
	"version": 2,
	"builds": [
		{
			"src": "server/index.ts",
			"use": "@vercel/node"
		},
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist/public"
			}
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "server/index.ts"
		},
		{
			"src": "/models/(.*)",
			"dest": "/models/$1"
		},
		{
			"src": "/assets/(.*)",
			"dest": "/assets/$1"
		},
		{
			"src": "/(.*\\.[a-z]+)$",
			"dest": "/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	]
}
